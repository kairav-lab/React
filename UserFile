function User(){
    return (
        <h1>Hello</h1>
        <h1>{10+20}</h1>
    )
}
export default User;

import React from 'react'
function User(){
    return React.createElement('div',null,React.createElement('h1',null,"Hello inner tag"))
}
export default User;
